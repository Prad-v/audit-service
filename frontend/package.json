{
  "name": "audit-log-frontend",
  "version": "1.0.0",
  "description": "Backstage frontend for the audit log framework",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "backstage-cli package start",
    "start": "backstage-cli package start",
    "build": "yarn workspace @internal/app build",
    "build:backend": "backstage-cli package build --role backend",
    "build:image": "backstage-cli package build-image",
    "tsc": "tsc",
    "tsc:full": "tsc --skipLibCheck false --incremental false",
    "clean": "backstage-cli package clean",
    "test": "backstage-cli package test",
    "test:watch": "backstage-cli package test --watch",
    "test:e2e": "cross-env PORT=3001 start-server-and-test start http://localhost:3001 cy:dev",
    "test:e2e:ci": "cross-env PORT=3001 start-server-and-test start http://localhost:3001 cy:run",
    "lint": "backstage-cli package lint",
    "lint:fix": "backstage-cli package lint --fix",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "cy:dev": "cypress open",
    "cy:run": "cypress run --browser chrome"
  },
  "dependencies": {
    "@backstage/app-defaults": "^1.4.7",
    "@backstage/catalog-model": "^1.4.3",
    "@backstage/cli": "^0.22.13",
    "@backstage/core-app-api": "^1.11.3",
    "@backstage/core-components": "^0.13.10",
    "@backstage/core-plugin-api": "^1.8.2",
    "@backstage/integration-react": "^1.1.25",
    "@backstage/plugin-org": "^0.6.22",
    "@backstage/plugin-permission-react": "^0.4.17",
    "@backstage/plugin-search": "^1.4.7",
    "@backstage/plugin-user-settings": "^0.8.0",
    "@backstage/theme": "^0.4.4",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "4.0.0-alpha.61",
    "history": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-use": "^17.2.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "typescript": "~4.6.4"
  },
  "devDependencies": {
    "@backstage/test-utils": "^1.4.7",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^16.11.26",
    "cross-env": "^7.0.0",
    "cypress": "^13.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.0.0",
    "prettier": "^2.3.2",
    "start-server-and-test": "^2.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@azure/storage-blob": "12.17.0",
    "@azure/logger": "1.3.0",
    "@azure/core-util": "1.13.0",
    "@azure/core-auth": "1.10.0"

  },
  "files": [
    "dist"
  ],
  "backstage": {
    "role": "frontend"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "plugins/*"
    ]
  }
}